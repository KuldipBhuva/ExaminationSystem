@using ERP_Models.ViewModel
@using ERP_Models

@{
    ERPEntities Dbcontext = new ERPEntities();
     int did = 0;
            int acid = 0;
            if (Session["DID"] != null)
            {
                did = Convert.ToInt32(Session["DID"].ToString());
                acid = Convert.ToInt32(Session["ACID"].ToString());
